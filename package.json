    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.8"
  },
  "peerDependencies": {
    "svelte": "^3.44.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svelte-drama/suspense"
